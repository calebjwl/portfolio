<div class="section">
  <div class="container">
    <div class="title-block">
      <p class="title centered">Music</p>
      <span class="music-links">
        <a target="_blank" class="hvr-dark" href="http://instagram.com/calebjwl">{{fa-icon "instagram"}}</a>
        <a target="_blank" class="hvr-dark" href="http://youtube.com/calebjwl">{{fa-icon "youtube"}}</a>
      </span>
    </div>
    <div class="grid">
      {{#each items as |item|}}
        <div class="grid__item">
          <a href="{{item.link}}" target="_blank" class="thumbnail">
          <div class="card hvr-card-toggle">
            <img class="" src={{item.src}}/>
            <div class="hovered-card">
              <p class="card__title">{{item.title}}</p>
              <p class="card__description">{{item.description}}</p>
              <div class="card__info">
                <p class="card__tools">{{item.tools}}</p>
              </div>
            </div>
          </div>
          </a>
        </div>
      {{/each}}
    </div>
  </div>
</div>
